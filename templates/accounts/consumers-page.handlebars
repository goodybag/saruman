<div class="row page-actions">
  <div class="input-append search-wrapper">
    <input class="span2" placeholder="Search by Email" class="users-search" type="text">
    <button class="btn" type="button">
      <i class="icon-search"></i>
    </button>
  </div>
  <div class="users-paginator-top"></div>
</div>
<table class="table table-striped users-table">
  <thead>
    <tr>
      <th class="id">Id</th>
      <th class="firstName">First Name</th>
      <th class="lastName">Last Name</th>
      <th class="screenName">Screen Name</th>
      <th class="cardId">Card ID</th>
      <th class="avatarUrl">Avatar</th>
      <th class="actions">
        <button class="btn btn-primary btn-new-user">
          <i class="icon-plus-sign-alt"></i>
          New Consumer
        </button>
      </th>
    </tr>
  </thead>
  <tbody class="users-list"></tbody>
</table>

<div class="users-paginator-bottom"></div>