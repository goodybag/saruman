<div class="navbar navbar-fixed-top navbar-inverse">
  <div class="navbar-inner">
    <a class="brand" href="#panel/dashboard" class="msg">Gerdyberg Biz Panel</a>
    <ul class="nav pull-right">
      <li><a href="#" class="msg" data-msg-name="debugLoadLocationProducts" data-location-id="{{location.id}}">Load Location Products</a></li>
      <li><a href="#" class="msg" data-msg-name="loadMenuBegin" data-business-id="{{business.id}}" data-location-id="{{location.id}}">Load Menu</a></li>
      <li><a href="#logout">Logout</a></li>
    </ul>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <span class="business-name">{{business.name}}</span>
    <span class="caret-sep">&gt;&gt;</span>
    {{#if multipleLocations}}
    <span class="location dropdown">
      <button class="btn btn-primary btn-large dropdown-toggle" data-toggle="dropdown" href="#">{{location.street1}} <span class="caret"></span></button>
      <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
        {{#each business.locations}}
        <li class="{{#active}}active{{/active}}">
        <a tabindex="-1" href="#" class="msg" data-msg-name="changeLocation" data-location-id="{{id}}">{{street1}}</a>
        </li>
        {{/each}}
      </ul>
    </span>
    {{else}}
    <span class="location-name">{{location.street1}}</span>
    {{/if}}
  </div>
</div>
<div class="row-fluid">
  <div class="span-12">
  </div>
</div>
