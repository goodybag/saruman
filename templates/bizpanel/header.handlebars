<div class="navbar navbar-fixed-top navbar-inverse">
  <div class="navbar-inner" style="padding-left:20px;">
    <a class="brand" href="#/dashboard" class="msg">{{business.name}}</a>
    {{#if multipleLocations}}
    <span class="location dropdown">
      <button class="btn  dropdown-toggle" data-toggle="dropdown" href="#">Location: {{location.street1}} <span class="caret"></span></button>
      <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
        {{#each business.locations}}
        <li class="{{#active}}active{{/active}}">
        <a tabindex="-1" href="#" class="msg" data-msg-name="changeLocation" data-location-id="{{id}}">{{street1}}</a>
        </li>
        {{/each}}
      </ul>
    </span>
    {{else}}
    {{/if}}
    <ul class="nav pull-right">
      <li><a href="#" class="msg" data-msg-name="loadMenuBegin">Load Menu</a></li>
      <li><a href="#" class="msg" data-msg-name="loadManagerBegin" data-business-id="{{business.id}}" data-location-id="{{location.id}}">Load Manager</a></li>
      <li><a href="#/logout">Logout</a></li>
    </ul>
  </div>
</div>
<div class="row-fluid">
  <div class="span-12">
  </div>
</div>
