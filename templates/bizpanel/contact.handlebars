<div class="row-fluid">
  <div class="span6">
    <center>
      <h3><a href="mailto:support@goodybag.com">support@goodybag.com</a></h3>
      <h3>512-667-4224</h3>
    </center>
    <div class="row-fluid">
      <div class="span6">
        <button class="btn btn-block">Request your acct. Manager</button>
      </div>
      <div class="span6">
        <button class="btn btn-block">Request Keytags</button>
      </div>
    </div>
  </div>
  <div class="span6">
    <img src="http://austinot.com/wp-content/uploads/2013/01/Goodybag-Team-Austin-TX-1024x682.jpg" />
  </div>
</div>
<div class="row-fluid">
  <div class="span8">
    <form id="contact" class="form contact">
      <div class="row">
        <div class="span2">
          <label for="contact-name">Name</label>
        </div>
        <div class="span10">
          <input type="text" name="name" id="contact-name" placeholder="Your Name"/>
        </div>
      </div>
      <div class="row">
        <div class="span2">
          <label for="contact-email">Email Address</label>
        </div>
        <div class="span10">
          <input type="email" name="from" id="contact-email" value="{{user.email}}" />
        </div>
      </div>
      <div class="row">
        <div class="span2">
          <label for="contact-subject">Subject</label>
        </div>
        <div class="span10">
          <input type="text" id="contact-subject" name="subject" />
        </div>
      </div>
      <div class="row">
        <div class="span2">
          <label for="contact-message">Message</label>
        </div>
        <div class="span10">
          <textarea id="contact-message" name="message"></textarea>
        </div>
      </div>
      <div class="row">
        <div class="span2"></div>
        <div class="span10">
          <label class="checkbox">
            <input type="checkbox" id="contact-copy" name="copySender" />Send me a copy
          </label>
        </div>
      </div>
      <div class="row">
        <div class="span2"></div>
        <div class="span10">
          <button class="btn msg" data-msg-name="sendContactMessage">Send</button>
          <span class="message hide">Sending...</span>
        </div>
      </div>
    </form>
  </div>
</div>
